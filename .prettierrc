{
    "printWidth": 100,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "endOfLine": "lf",
    "insertPragma": false,

    "plugins": ["prettier-plugin-astro"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ]
}
